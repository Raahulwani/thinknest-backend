{
  "name": "thinknest-backend-jury-starter",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "seed": "ts-node src/db/seed/seed.ts",
    "seed:hof": "ts-node src/db/seed/seed-hof.ts",
    "seed:featured": "ts-node src/db/seed/seed-featured.ts",
    "seed:challenges": "ts-node src/db/seed/seed-challenges.ts",
    "seed:news": "ts-node src/db/seed/seed-news.ts",
    "seed:case-studies": "ts-node ./src/db/seed/seed.case-studies.ts",
    "seed:media": "ts-node src/db/seed/seed-media-stories.ts"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "multer": "^2.0.2",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^2.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
